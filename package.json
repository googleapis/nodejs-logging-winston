    "c8": "^10.1.3",
    "codecov": "^3.8.3",
    "gts": "^6.0.2",
    "jsdoc": "^4.0.5",
    "jsdoc-fresh": "^5.0.2",
    "jsdoc-region-tag": "^4.0.1",
    "linkinator": "^6.0.0",
    "mocha": "^11.7.5",
    "pack-n-play": "4.2.1",
